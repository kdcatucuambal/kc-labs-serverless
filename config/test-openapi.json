{
  "parameters": {
    "author": "Kevin Catucuamba",
    "email": "catucuambakevin@gmail.com",
    "EnvironmentId": "oas",
    "Module": "Labs"
  },
  "pipeline": {
    "project": {
      "project-key": "kc-lab-serverless",
      "path-service": "kc-labs-app",
      "backend-branch": "develop"
    },
    "deploy": {
      "stack-name": "${EnvironmentId}-labs-serverless",
      "s3-bucket": "dev1-kc-labs-serverless",
      "prefix": "${EnvironmentId}",
      "aws-default-region": "us-east-1",
      "capabilities": "CAPABILITY_AUTO_EXPAND CAPABILITY_IAM CAPABILITY_NAMED_IAM",
      "non-confirm-change-set": "true",
      "parameters": [
        {
          "parameterKey": "EnvironmentId",
          "parameterValue": "${EnvironmentId}"
        },
        {
          "parameterKey": "LambdaAliasProduction",
          "parameterValue": "production"
        },
        {
          "parameterKey": "LambdaAliasBeta",
          "parameterValue": "beta"
        },
        {
          "parameterKey": "LogRetentionInDays",
          "parameterValue": 5
        },
        {
          "parameterKey": "LambdaAuthorizer",
          "parameterValue": "dev1-AuthorizerLambda"
        },
        {
          "parameterKey": "ProvisionedConcurrencyConfigEnabled",
          "parameterValue": "false"
        },
        {
          "parameterKey": "CustomDomainName",
          "parameterValue": "labs-${EnvironmentId}.megapro.site"
        },
        {
          "parameterKey": "PublicCertificateArn",
          "parameterValue": "arn:aws:acm:us-east-1:818802851925:certificate/7799a8b0-a3fe-4820-a645-e883cfb599d1"
        }
      ]
    }
  }
}